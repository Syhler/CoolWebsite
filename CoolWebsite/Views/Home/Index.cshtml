@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}



<!DOCTYPE html>
<html lang="dk">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Home - Brand</title>

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,700,700i,600,600i">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.10.0/baguetteBox.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Actor">
    <link rel="stylesheet" href="~/lib/test/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/fonts/font-awesome.min.css">
    <link rel="stylesheet" href="~/fonts/ionicons.min.css">
    <link rel="stylesheet" href="~/fonts/simple-line-icons.min.css">
    <link rel="stylesheet" href="~/lib/css/Profile-Card.css">
    <link rel="stylesheet" href="~/css/site.css"/>
    @*
    <link rel="stylesheet" href="assets/css/Profile-Picture-With-Badge-1.css">
    <link rel="stylesheet" href="assets/css/Profile-Picture-With-Badge.css">
    *@
    <link rel="stylesheet" href="~/lib/css/smoothproducts.css">


    <script src="~/lib/jquery/dist/jquery.min.js"></script>


</head>

<body>
<nav class="navbar navbar-light navbar-expand-lg fixed-top bg-white clean-navbar">
    <div class="container">
        <a class="navbar-brand logo" href="#">MSyhler</a>
        <button data-toggle="collapse" class="navbar-toggler" data-target="#navcol-1">
            <span class="sr-only">Toggle navigation</span><span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse"
             id="navcol-1">
            <ul class="nav navbar-nav ml-auto">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="home">Home</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="who-am-i">Who am i</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="courses">Courses</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="skill-set">Skill sets</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="projects">Projects</a>
                </li>
                @{
                    bool isAuthenticated = User.Identity.IsAuthenticated;
                    if (isAuthenticated)
                    {
                      
                        
                        <li class="nav-item" role="presentation">
                            <a class="nav-link" asp-area="Financial" asp-controller="Home" asp-action="Index">Financial</a>
                        </li>
                        
                    }
                    else
                    {
                        <li class="nav-item" role="presentation">
                            <a class="nav-link" id="sign-in">Sign in</a>
                        </li>
                        
                    }
                }
                
            </ul>
        </div>
    </div>
    
    @{
        if (isAuthenticated)
        {
            <div style="float: left">
            <a class="link" asp-area="" asp-controller="Auth" asp-action="Logout">@User.Identity.Name : Sign out</a>
            </div>
        }
    }
</nav>
<main class="page landing-page">
    <section class="clean-block clean-hero main-image home" style="color:rgba(9, 162, 255, 0.85); ">
        <div class="text">
            <h2>Morten Syhler Portfolio</h2>
            <p>.NET developer</p><button class="btn btn-outline-light btn-lg" type="button">Learn More</button>
        </div>
    </section>
    <section class="clean-block clean-info dark who-am-i">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Who Am I</h2>
                <p>My name is Morten Syhler. I'm 21 years old. I have <br>~ 2 years of professional experience as a <br>.NET full stack developer&nbsp;<br></p>
            </div>
            <div class="row align-items-center">
                <div class="col text-center">
                    <img class="img-thumbnail" src="~/img/Homepage/image-250nw-575234476.jpg" width="500">
                </div>
                <div class="col-md-6">
                    <h3>What i do</h3>
                    <div class="getting-started-info">
                        <p>I am currently attending college in denmark. Currently on my 3rd semester. Been programming since i was 17.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="clean-block courses">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Courses</h2>
                <p>A list of what courses i have taken.</p>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-5 feature-box">
                    <i class="icon-star icon"></i>
                    <h4>C#</h4>
                    <ul class="list-unstyled">
                        <li>C# Basics. By <strong>Code with Mosh</strong></li>
                        <li>C# Intermediate. By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>C# Advance. By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>ASP.NET MVC 5. By&nbsp;<strong>Code with Mosh</strong><br><br></li>
                    </ul>
                </div>
                <div class="col-md-5 feature-box">
                    <i class="icon-pencil icon"></i>
                    <h4>CSS - HTML</h4>
                    <ul class="list-unstyled">
                        <li>Build Responsvie Website with HTML5 and CSS3 By <strong>Jonas Schemdtmann</strong></li>
                    </ul>
                </div>
                <div class="col-md-5 feature-box">
                    <i class="icon ion-android-bulb icon"></i>
                    <h4>General</h4>
                    <ul class="list-unstyled">
                        <li>Unit Testing.&nbsp;By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>Clean Coding and Refactoring. By&nbsp;<strong>Code with Mosh</strong><br><br></li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section class="clean-block skill-set dark pb-3">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Skill Set</h2>
                <p>List of my programming skills</p>
            </div>
            <ul class="list-unstyled">
                <li>
                    <div>
                        <p>
                            <img src="~/img/Homepage/C-Sharp-01.png" width="35" height="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100" style="width: 85%;">85%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/HTML5_Logo_128.png" width="35" height="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width: 70%;">70%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/Mmww2.png" width="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="65" aria-valuemin="0" aria-valuemax="100" style="width: 65%;">65%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/CSS3_logo_and_wordmark.svg.png" width="30">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-warning" aria-valuenow="55" aria-valuemin="0" aria-valuemax="100" style="width: 55%;">55%</div>
                        </div>
                    </div>
