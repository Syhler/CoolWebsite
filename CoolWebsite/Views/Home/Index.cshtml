@{
    ViewData["Title"] = "Home Page";
    Layout = "_HomePageLayout";
}


@section NavBar
{
 <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="home">Home</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="who-am-i">Who am i</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="courses">Courses</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="skill-set">Skill sets</a>
                </li>
<li class="nav-item" role="presentation">
    <a class="nav-link" id="projects">Projects</a>
</li>

}




<main class="page landing-page" style="padding-top: 0;">
    <section class="clean-block clean-hero main-image home" style="color:rgba(9, 162, 255, 0.85); ">
        <div class="text">
            <h2>Morten Syhler Portfolio</h2>
            <p>.NET developer</p>
            <button class="btn btn-outline-light btn-lg" type="button" id="learn-more">Learn More</button>
        </div>
    </section>
    <section class="clean-block clean-info dark who-am-i">
        <div class="container">
            <div class="block-heading">
                
                <h2 class="text-info">Who Am I</h2>
                <p>My name is Morten Syhler. I'm 21 years old. I have <br>~ 2 years of professional experience as a <br>.NET full stack developer&nbsp;<br></p>
            </div>
            <div class="row align-items-center">
                <div class="col text-center">
                    <img class="img-thumbnail round-image" src="~/img/Homepage/profile_pic.png" width="500">
                </div>
                <div class="col-md-6">
                    <h3></h3>
                    <div class="getting-started-info">
                        <p>I am currently attending college in Denmark. I am in my 3rd semester. I have Been programming since I was 17.
                            I have a strong passion for Software Development. I do always try to keep up with the latest technologies.
                            I am driven, by nicely structured code and easily readable, I do find that quite rewarding.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="clean-block courses">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Courses</h2>
                <p>A list of what courses i have taken.</p>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-5 feature-box">
                    <i class="icon-star icon"></i>
                    <h4>C#</h4>
                    <ul class="list-unstyled">
                        <li>C# Basics. By <strong>Code with Mosh</strong></li>
                        <li>C# Intermediate. By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>C# Advance. By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>ASP.NET MVC 5. By&nbsp;<strong>Code with Mosh</strong><br><br></li>
                    </ul>
                </div>
                <div class="col-md-5 feature-box">
                    <i class="icon-pencil icon"></i>
                    <h4>CSS - HTML</h4>
                    <ul class="list-unstyled">
                        <li>Build Responsvie Website with HTML5 and CSS3 By <strong>Jonas Schemdtmann</strong></li>
                    </ul>
                </div>
                <div class="col-md-5 feature-box">
                    <i class="icon ion-android-bulb icon"></i>
                    <h4>General</h4>
                    <ul class="list-unstyled">
                        <li>Unit Testing.&nbsp;By&nbsp;<strong>Code with Mosh</strong><br></li>
                        <li>Clean Coding and Refactoring. By&nbsp;<strong>Code with Mosh</strong><br><br></li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section class="clean-block skill-set dark pb-3">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Skill Set</h2>
                <p>List of my programming skills</p>
            </div>
            <ul class="list-unstyled">
                <li>
                    <div>
                        <p>
                            <img src="~/img/Homepage/C-Sharp-01.png" width="35" height="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100" style="width: 90%;">90%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/HTML5_Logo_128.png" width="35" height="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 90%;">90%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/Mmww2.png" width="35">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-success" aria-valuenow="65" aria-valuemin="0" aria-valuemax="100" style="width: 70%;">70%</div>
                        </div>
                    </div>
                </li>
                <li class="mt-3">
                    <div>
                        <p>
                            <img src="~/img/Homepage/CSS3_logo_and_wordmark.svg.png" width="30">
                        </p>
                        <div class="progress">
                            <div class="progress-bar bg-warning" aria-valuenow="55" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">60%</div>
                        </div>
                    </div>
                </li>
            </ul>
            <div class="block-heading pt-2">
                <h2 class="text-info">Other Programming skills</h2>
            </div>
        </div>
        <ul class="list-inline text-center">
            <li class="list-inline-item">MySQL<img src="~/img/Homepage/logo-mysql-170x115.png" width="50"></li>
            <li class="list-inline-item">SQL<img src="~/img/Homepage/pngwing.com.png" width="40"></li>
            <li class="list-inline-item">Java<img src="~/img/Homepage/58480979cef1014c0b5e4901.png" width="30"></li>
            <li class="list-inline-item">Kotlin<img src="~/img/Homepage/kotlin-1-logo-png-transparent.png" width="35"></li>
        </ul>
    </section>
    <section class="clean-block projects">
        <div class="container">
            <div class="block-heading">
                <h2 class="text-info">Projects</h2>
                <p>Some project of mine</p>
            </div>
            <div>
                <div class="row">
                    <div class="col">
                        <div class="profile-card">
                            <div class="profile-back"></div><img class="rounded-circle profile-pic" src="~/img/Homepage/Untitled.png">
                            <h3 class="profile-name">CoolWebsite - (Current Website)</h3>
                            <p class="profile-bio">
                                It's called CoolWebsite simply because of the lack of a better name. However, I've created the website to make it easier for my roommates and me to split receipts from grocery stores.<br>To try out the application, sign
                                in with Guest123@guest.com as the email and Guest123 as the password. <br>Click Read More to read about the plans for this project and more. <br>The source code to the project is available by clicking on the GitHub
                                repo.
                            </p>
                            <div class="mr-4 ml-4">
                                <a class="btn btn-outline-primary btn-block" asp-action="CoolWebsiteProject" asp-controller="Home" asp-area="">Read More</a>
                            </div>
                            <div class="mt-2">
                                <ul class="list-inline">
                                    <li class="list-inline-item text-primary" style="font-size: 40px;">
                                        <a href="https://github.com/Syhler/CoolWebsite" target="_blank">
                                            <i class="fa fa-github"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col">
                        <div class="profile-card">
                            <div class="profile-back"></div><img class="rounded-circle profile-pic" src="~/img/Homepage/app_icon.png">
                            <h3 class="profile-name">Messaging Android App</h3>
                            <p class="profile-bio">
                                Messaging App does exactly what the name says. I've created the app in 51 hours, which includes learning android development. I did use firebase as the backend. It's possible to see the app in action by clicking on the
                                GitHub repo.
                            </p>
                            <div class="mr-4 ml-4">
                                <button class="btn btn-outline-primary btn-block" type="button" disabled="">Read More</button>
                            </div>
                            <div class="mt-2">
                                <ul class="list-inline">
                                    <li class="list-inline-item text-primary" style="font-size: 40px;">
                                        <a href="https://github.com/Syhler/MessagingApp" target="_blank">
                                            <i class="fa fa-github"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>


@section Scripts
{
    <script src="~/js/homepage/homepage.js"></script>
}



